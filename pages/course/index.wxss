.swiper-tab {
	width: 100%;
	border-bottom: 2rpx solid #edeeef;
	text-align: center;
	line-height: 80rpx;
}
.swiper-tab-list {  
  font-size: 17px;
  display: inline-block;
	width: 33.33%;
	color: #cccccc;
}
.on { 
  color: #33cb67;
	border-bottom: 5rpx solid #33cb67;
}
.swiper-box { 
  display: block; 
  height: 100%; 
  width: 100%; 
  overflow: visible;
}
swiper-item {
  height: 200px;
  overflow: visible;
}
.course {
  display:flex;
  height: 50px;
  align-items:center;
  flex-direction: row;
  padding-left: 15px;
  margin-top: 7px;
}
.course-img {
  width:  45px;
  height: 45px;
  display: flex;
  align-items:center;
  background-size:100% 100%;
  background-image: url('data:image/jpeg;base64,');
}
.course-img image{
  width: 45px;
  height: 45px;
  border-radius: 9px;
  overflow: hidden;
  position: relative;
}
.course-text {
  font-size: 18px;
  padding-left: 15px;
}
.flex-container {
  display:flex;
  flex-direction: column;
}
.course-hover {
  background: #efefef !important;
}
#ld-block{
    width: 100vw;
    height: 100vh;
    display: flex;
    justify-content:center; 
    align-items:center;
}
.ld-img {
  width: 110px;
}
.show {
 display: flex !important;
}
.ldhide {
 display: none !important;
}
.course-desp-wrap {
  padding: 20px;
}
.course-name {
  text-align: left !important;
  line-height: 40px;
  font-weight: bold;
}
.course-desp {
  font-size: 14px;
  text-align: justify !important;
}
.flex-view-item {
   width: 100vw;
}
.comment-wrap {
  padding-left: 5vw;
  padding-right: 5vw;
  padding-top: 20px;
}
.comment {
  display:flex;
  height: auto;
  align-items:center;
  flex-direction: row;
  margin-bottom: 10px;
}
.comment-title {
  padding-left: 5vw;
  font-size: 15px;
  padding-top: 20px;
  font-style: italic;
  color: #abacad;
}
.comment-img {
  width:  25vw;
  display:flex;
  align-items:center;
}
.comment-text {
  width:  65vw;
  font-size: 12px;
  font-style: italic;
  color: #abacad;
}
.comment-img image {
  width: 50px;
  height: 50px;
  margin: 0 auto;
  border-radius: 9px;
  overflow: hidden;
  position: relative;
}
.comment-name {
  text-align: right;
  margin-right: 10px;
}
.place1 {
  display:flex;
  height: 50px;
}
.place2 {
  display:flex;
  height: 20px;
}
.place3 {
  display:flex;
  height: 50px;
}
